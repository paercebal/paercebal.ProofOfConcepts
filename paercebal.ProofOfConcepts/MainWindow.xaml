<Window x:Class="paercebal.ProofOfConcepts.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:paercebal.ProofOfConcepts"
        xmlns:pocs="clr-namespace:paercebal.ProofOfConcepts.Pocs"
        mc:Ignorable="d"
        Title="MainWindow" Height="500" Width="800" Background="#66AAFF"
        KeyDown="OnKeyDown"
        >
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <WrapPanel x:Name="PanelForButtons" Grid.Row="0" Grid.Column="0">
        </WrapPanel>
        <Grid x:Name="GridForExperiments" Grid.Row="1" Grid.Column="0">
        </Grid>
    </Grid>
</Window>
